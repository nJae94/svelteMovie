{
	"name": "svelte-movie",
	"version": "0.0.1",
	"description": "sveltekit & electron test",
	"main": "src/electron.cjs",
	"scripts": {
		"dev": "cross-env NODE_ENV=dev concurrently -n=svelte,electron -c='#ff3e00',blue \"npm run dev:svelte\" \"npm run dev:electron\"",
		"dev:svelte": "svelte-kit dev",
		"dev:electron": "electron src/electron.cjs",
		"build:svelte": "svelte-kit build",
		"build:electron": "electron-builder -mw --config electron.build.config.json",
		"build": "cross-env NODE_ENV=production npm run build:svelte && npm run build:electron",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"browserslist": [
		"> 1%",
		"last 2 versions"
	],
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-static": "^1.0.0-next.28",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"autoprefixer": "^10.4.2",
		"concurrently": "^7.0.0",
		"cross-env": "^7.0.3",
		"dotenv": "^16.0.0",
		"electron": "^17.0.0",
		"electron-builder": "^22.14.13",
		"electron-connect": "^0.6.3",
		"electron-packager": "^15.4.0",
		"electron-updater": "^4.6.5",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"lodash": "^4.17.21",
		"postcss": "^8.4.6",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"sass": "^1.49.7",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.9.4",
		"tslib": "^2.3.1",
		"typescript": "^4.4.3"
	},
	"type": "module",
	"dependencies": {
		"electron-context-menu": "^3.1.1",
		"electron-reloader": "^1.2.2",
		"electron-serve": "^1.1.0",
		"electron-window-state": "^5.0.3"
	}
}
